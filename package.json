{
  "name": "@mtti/uuid",
  "version": "1.0.0",
  "description": "Branded UUID types for Typescript",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.11.1"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/index.js",
    "build": "tsc",
    "jest": "jest --config ./jest.config.json",
    "lint": "npm run prettier:check && eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier:check": "prettier ./src --check",
    "prettier:fix": "prettier ./src --write",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "setup": "npm install",
    "test": "npm run lint && npm run jest",
    "test:docker": "docker build -f ./Dockerfile-test -t ts-test . && docker run --rm -it ts-test",
    "tsc": "tsc",
    "watch": "chokidar --initial -c \"npm run build\" \"src/**/*.ts\""
  },
  "files": [
    "lib",
    "!lib/**/*.test.*"
  ],
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "prettier": "3.3.2",
    "ts-jest": "29.1.4",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "uuid": "^10.0.0"
  }
}
